if game.PlaceId == 6284583030 then
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Space Scripts", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local TradeTab = Window:MakeTab({
	Name = "Trade Scam",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
  })
  
  TradeTab:AddButton({
	Name = "Enable Trade Scam!",
	Callback = function()
      		print("Trade scam activated!")
  	end    
})

TradeTab:AddButton({
	Name = "Disable Trade Scam!",
	Callback = function()
      		print("Trade scam Disabled!")
  	end    
})

local DupeTab = Window:MakeTab({
	Name = "Dupe",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
  })
  
DupeTab:AddButton({
	Name = "Start Dupe",
	Callback = function()
      		print("Dupe Starting Disabled!")
})

DupeTab:AddDropdown({
	Name = "Select To Dupe",
	Default = "Gems",
	Options = {"Gems", "Pets"},
	Callback = function(Value)
		print(Value)
	end    
})

end
OrionLib:Init()
